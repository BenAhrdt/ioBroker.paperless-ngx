  "objects": [],
  "instanceObjects": [
    {
      "_id": "control",
      "type": "channel",
      "common": {
        "name": "control",
        "icon": "img/control.png"
      },
      "native": {}
    },
    {
      "_id": "control.requestTrigger",
      "type": "state",
      "common": {
        "name": "requests the configed Types",
        "type": "boolean",
        "role": "button",
        "icon": "img/requestTrigger.png",
        "default": false
      },
      "native": {}
    },
    {
      "_id": "search",
      "type": "channel",
      "common": {
        "name": "search",
        "icon": "img/search.png"
      },
      "native": {}
    },
    {
      "_id": "search.global",
      "type": "folder",
      "common": {
        "name": "global search"
      },
      "native": {}
    },
    {
      "_id": "search.global.query",
      "type": "state",
      "common": {
        "name": "query of global search",
        "type": "string",
        "role": "text",
        "icon": "img/query.png",
        "default": ""
      },
      "native": {}
    },
    {
      "_id": "search.documents",
      "type": "folder",
      "common": {
        "name": "document search"
      },
      "native": {}
    },
    {
      "_id": "search.documents.query",
      "type": "state",
      "common": {
        "name": "query of document search",
        "type": "string",
        "role": "text",
        "icon": "img/query.png",
        "default": ""
      },
      "native": {}
    },
    {
      "_id": "search.documents.queryTags",
      "type": "state",
      "common": {
        "name": "query of document search",
        "type": "array",
        "role": "value",
        "icon": "img/query.png",
        "default": []
      },
      "native": {}
    },
    {
      "_id": "search.documents.queryBlockedTags",
      "type": "state",
      "common": {
        "name": "query of document search (blocked tags)",
        "type": "array",
        "role": "value",
        "icon": "img/query.png",
        "default": []
      },
      "native": {}
    },
    {
      "_id": "search.documents.queryAllTags",
      "type": "state",
      "common": {
        "name": "Logic for search: true => And, false => or",
        "type": "boolean",
        "role": "switch",
        "icon": "img/query.png",
        "default": true
      },
      "native": {}
    },
    {
      "_id": "info",
      "type": "channel",
      "common": {
        "name": "Information"
      },
      "native": {}
    },
    {
      "_id": "info.connection",
      "type": "state",
      "common": {
        "role": "indicator.connected",
        "name": "Device or service connected",
        "type": "boolean",
        "read": true,
        "write": false,
        "def": false
      },
      "native": {}
    },
    {
      "_id": "info.currentStep",
      "type": "state",
      "common": {
        "role": "indicator",
        "name": "current step in the adapter",
        "type": "string",
        "read": true,
        "write": false,
        "def": "starting",
        "icon": "img/currentStep.png"
      },
      "native": {}
    }
  ]
}
