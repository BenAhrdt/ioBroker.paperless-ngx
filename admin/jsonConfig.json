{
	"i18n": true,
	"type": "tabs",
	"items":{
        "mainTab":{
            "type": "panel",
            "label": "mainSettings",
			"items": {
				"_serversettingsHeader": {
					"newLine": true,
					"type": "header",
					"text": "ServersettingsHeader",
					"size": 3
				},
				"_serverinformation":{
					"newLine":true,
					"type": "staticText",
					"label": "Serverinformation"
				},
				"ipUrl":{
					"newLine": true,
					"type": "text",
					"label": "ipUrl",
					"tooltip": "ipUrlTooltip",
					"default": "",
					"sm": 4
				},
				"port":{
					"type": "number",
					"label": "port",
					"tooltip": "portTooltip",
					"default": 8888,
					"sm": 1
				},
				"_authenticationHeader": {
					"newLine": true,
					"type": "header",
					"text": "AuthenticationHeader",
					"size": 3
				},
				"_authenticationInformation":{
					"newLine":true,
					"type": "staticText",
					"label": "AuthenticationInformation"
				},
				"username":{
					"newLine":true,
					"type": "text",
					"label": "username",
					"tooltip": "usernameTooltip",
					"default": "",
					"sm": 4
				},
				"password":{
					"type": "password",
					"label": "password",
					"tooltip": "passwordTooltip",
					"default": "",
					"sm": 3
				}
			}
		},
		"dataTab":{
            "type": "panel",
            "label": "dataSettings",
			"items": {
				"_datasettingsHeader": {
					"newLine": true,
					"type": "header",
					"text": "DatasettingsHeader",
					"size": 3
				},
				"_datainformation":{
					"newLine":true,
					"type": "staticText",
					"label": "Datainformation"
				},
				"refreshCycle": {
                    "newLine": true,
                    "type": "cron",
                    "complex": true,
                    "label": "refreshCycle",
                    "tooltip": "refreshCycleTooltip",
                    "sm":2,
                    "default": "* * * * *"
				},
				"documentsLoglevel":{
					"newLine": true,
					"type": "select",
					"label": "documentsLoglevel",
					"tooltip": "documentsLoglevelTooltip",
					"options": [
						{"label":"without","value":0},
						{"label":"Basic","value":1},
						{"label":"Detailed","value":2}
					],
					"default": "without",
					"sm":2
				},
				"documentTypesLoglevel":{
					"newLine": true,
					"type": "select",
					"label": "documentTypesLoglevel",
					"tooltip": "documentTypesLoglevelTooltip",
					"options": [
						{"label":"without","value":0},
						{"label":"Basic","value":1},
						{"label":"Detailed","value":2}
					],
					"default": "without",
					"sm":2
				},
				"tagsLoglevel":{
					"newLine": true,
					"type": "select",
					"label": "tagsLoglevel",
					"tooltip": "tagsLoglevelTooltip",
					"options": [
						{"label":"without","value":0},
						{"label":"Basic","value":1},
						{"label":"Detailed","value":2}
					],
					"default": "Detailed",
					"sm":2
				},
				"usersLoglevel":{
					"newLine": true,
					"type": "select",
					"label": "usersLoglevel",
					"tooltip": "usersLoglevelTooltip",
					"options": [
						{"label":"without","value":0},
						{"label":"Basic","value":1},
						{"label":"Detailed","value":2}
					],
					"default": "Basic",
					"sm":2
				}
			}
		}
	}
}